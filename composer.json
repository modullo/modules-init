{
    "name": "modullo/modules-init",
    "description": "Modullo Initialization Module",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ifeoluwa Olawoye",
            "email": "bolaji.olawoye@hostvillenigeria.org"
        }
    ],
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Modullo\\ModulesInit\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modullo\\ModulesInit\\ModulesInitServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
